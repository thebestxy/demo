<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dv.example.demo.DemoDAO">

  <select id="selectList">
    select
        id,
        name
    from
        demo
    <where>
      name = #{name}
    </where>
  </select>
</mapper>
